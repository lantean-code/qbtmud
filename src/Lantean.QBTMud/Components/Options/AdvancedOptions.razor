@inherits Options

<MudCard Elevation="1" Class="ml-4 mr-4 mb-4 mt-4">
    <MudCardHeader>
        <CardHeaderContent>
            <MudText Typo="Typo.subtitle2">@WebUiLocalizer.Translate("OptionsDialog", "qBittorrent Section")</MudText>
        </CardHeaderContent>
    </MudCardHeader>
    <MudCardContent Class="pt-0">
        <MudGrid>
            <MudItem xs="12">
                <MudSelect T="string" Label="@WebUiLocalizer.Translate("OptionsDialog", "Resume data storage type (requires restart):")" Value="ResumeDataStorageType" ValueChanged="ResumeDataStorageTypeChanged" Variant="Variant.Outlined" ShrinkLabel="true" data-test-id="@TestIdHelper.For("ResumeDataStorageType")">
                    <MudSelectItem T="string" Value="@("Legacy")">@WebUiLocalizer.Translate("OptionsDialog", "Fastresume files")</MudSelectItem>
                    <MudSelectItem T="string" Value="@("SQLite")">@WebUiLocalizer.Translate("OptionsDialog", "SQLite database (experimental)")</MudSelectItem>
                </MudSelect>
            </MudItem>
            <MudItem xs="12">
                <MudNumericField T="int" Label="@WebUiLocalizer.Translate("OptionsDialog", "Physical memory (RAM) usage limit:")" Value="MemoryWorkingSetLimit" ValueChanged="MemoryWorkingSetLimitChanged" Min="0" HelperText="@WebUiLocalizer.Translate("OptionsDialog", "This option is less effective on Linux")" Variant="Variant.Outlined" ShrinkLabel="true" Adornment="Adornment.End" AdornmentText="@WebUiLocalizer.Translate("misc", "MiB")" Immediate="true" DebounceInterval="300" data-test-id="@TestIdHelper.For("MemoryWorkingSetLimit")" />
            </MudItem>
            <MudItem xs="12">
                <MudSelect T="string" Label="@WebUiLocalizer.Translate("OptionsDialog", "Network interface:")" Value="CurrentNetworkInterface" ValueChanged="CurrentNetworkInterfaceChanged" Variant="Variant.Outlined" ShrinkLabel="true" data-test-id="@TestIdHelper.For("CurrentNetworkInterface")">
                    <MudSelectItem T="string" Value="@("")">@WebUiLocalizer.Translate("OptionsDialog", "Any interface")</MudSelectItem>
                    @foreach (var networkInterface in NetworkInterfaces)
                    {
                        <MudSelectItem T="string" Value="networkInterface.Value">@networkInterface.Name</MudSelectItem>
                    }
                </MudSelect>
            </MudItem>
            <MudItem xs="12">
                <MudSelect T="string" Label="@WebUiLocalizer.Translate("OptionsDialog", "Optional IP address to bind to:")" Value="CurrentInterfaceAddress" ValueChanged="CurrentInterfaceAddressChanged" Variant="Variant.Outlined" ShrinkLabel="true" data-test-id="@TestIdHelper.For("CurrentInterfaceAddress")">
                    <MudSelectItem T="string" Value="@("")">@WebUiLocalizer.Translate("OptionDialog", "All addresses")</MudSelectItem>
                    <MudSelectItem T="string" Value="@("0.0.0.0")">@WebUiLocalizer.Translate("OptionDialog", "All IPv4 addresses")</MudSelectItem>
                    <MudSelectItem T="string" Value="@("::")">@WebUiLocalizer.Translate("OptionDialog", "All IPv6 addresses")</MudSelectItem>
                    @foreach (var address in NetworkInterfaceAddresses)
                    {
                        <MudSelectItem T="string" Value="address">@address</MudSelectItem>
                    }
                </MudSelect>
            </MudItem>
            <MudItem xs="12">
                <MudNumericField T="int" Label="@WebUiLocalizer.Translate("OptionsDialog", "Save resume data interval:")" Value="SaveResumeDataInterval" ValueChanged="SaveResumeDataIntervalChanged" Min="0" Variant="Variant.Outlined" ShrinkLabel="true" Adornment="Adornment.End" AdornmentText="@WebUiLocalizer.Translate("OptionsDialog", "min")" Immediate="true" DebounceInterval="300" data-test-id="@TestIdHelper.For("SaveResumeDataInterval")" />
            </MudItem>
            <MudItem xs="12">
                <MudNumericField T="int" Label="@WebUiLocalizer.Translate("OptionsDialog", ".torrent file size limit:")" Value="TorrentFileSizeLimit" ValueChanged="TorrentFileSizeLimitChanged" Min="0" Variant="Variant.Outlined" ShrinkLabel="true" Adornment="Adornment.End" AdornmentText="@WebUiLocalizer.Translate("misc", "MiB")" Immediate="true" DebounceInterval="300" data-test-id="@TestIdHelper.For("TorrentFileSizeLimit")" />
            </MudItem>
            <MudItem xs="12">
                <FieldSwitch Label="@WebUiLocalizer.Translate("OptionsDialog", "Recheck torrents on completion:")" Value="RecheckCompletedTorrents" ValueChanged="RecheckCompletedTorrentsChanged" data-test-id="@TestIdHelper.For("RecheckCompletedTorrents")" />
            </MudItem>
            <MudItem xs="12">
                <MudNumericField T="int" Label="@WebUiLocalizer.Translate("OptionsDialog", "Refresh interval:")" Value="RefreshInterval" ValueChanged="RefreshIntervalChanged" Min="0" Variant="Variant.Outlined" ShrinkLabel="true" Adornment="Adornment.End" AdornmentText="@WebUiLocalizer.Translate("OptionsDialog", "ms")" Immediate="true" DebounceInterval="300" data-test-id="@TestIdHelper.For("RefreshInterval")" />
            </MudItem>
            <MudItem xs="12">
                <FieldSwitch Label="@WebUiLocalizer.Translate("OptionsDialog", "Resolve peer countries:")" Value="ResolvePeerCountries" ValueChanged="ResolvePeerCountriesChanged" data-test-id="@TestIdHelper.For("ResolvePeerCountries")" />
            </MudItem>
            <MudItem xs="12">
                <FieldSwitch Label="@WebUiLocalizer.Translate("OptionsDialog", "Reannounce to all trackers when IP or port changed:")" Value="ReannounceWhenAddressChanged" ValueChanged="ReannounceWhenAddressChangedChanged" data-test-id="@TestIdHelper.For("ReannounceWhenAddressChanged")" />
            </MudItem>
            <MudItem xs="12">
                <FieldSwitch Label="@WebUiLocalizer.Translate("OptionsDialog", "Enable embedded tracker:")" Value="EnableEmbeddedTracker" ValueChanged="EnableEmbeddedTrackerChanged" data-test-id="@TestIdHelper.For("EnableEmbeddedTracker")" />
            </MudItem>
            <MudItem xs="12">
                <MudNumericField T="int" Label="@WebUiLocalizer.Translate("OptionsDialog", "Embedded tracker port:")" Value="EmbeddedTrackerPort" ValueChanged="EmbeddedTrackerPortChanged" Min="@Options.MinPortValue" Max="@Options.MaxPortValue" Variant="Variant.Outlined" ShrinkLabel="true" Immediate="true" DebounceInterval="300" data-test-id="@TestIdHelper.For("EmbeddedTrackerPort")" />
            </MudItem>
            <MudItem xs="12">
                <FieldSwitch Label="@WebUiLocalizer.Translate("OptionsDialog", "Enable port forwarding for embedded tracker:")" Value="EmbeddedTrackerPortForwarding" ValueChanged="EmbeddedTrackerPortForwardingChanged" data-test-id="@TestIdHelper.For("EmbeddedTrackerPortForwarding")" />
            </MudItem>
        </MudGrid>
    </MudCardContent>
</MudCard>

