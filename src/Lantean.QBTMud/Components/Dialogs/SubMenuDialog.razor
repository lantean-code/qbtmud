<MudDialog ContentStyle="mix-width: 400px">
    <DialogContent>
        <MudGrid>
            <MudItem xs="12">
                <CascadingValue Value="Preferences">
                    <TorrentActions Hashes="Hashes" ParentAction="ParentAction" RenderType="RenderType.Children" MudDialog="MudDialog" Torrents="Torrents" Preferences="Preferences" Tags="Tags" Categories="Categories" />
                </CascadingValue>
            </MudItem>
        </MudGrid>
    </DialogContent>
    <DialogActions>
        <MudButton Style="display:none" OnClick="Cancel" data-test-id="@TestIdHelper.For("SubMenuCancel")">Cancel</MudButton>
        <MudButton Style="display:none" OnClick="CloseDialog" data-test-id="@TestIdHelper.For("SubMenuClose")">Close</MudButton>
    </DialogActions>
</MudDialog>
