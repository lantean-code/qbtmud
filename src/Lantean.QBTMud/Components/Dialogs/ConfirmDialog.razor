@inherits SubmittableDialog

<MudDialog>
    <DialogContent>
        @Content
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel" data-test-id="@TestIdHelper.For("ConfirmCancel")">@CancelText</MudButton>
        <MudButton Color="Color.Primary" OnClick="Submit" data-test-id="@TestIdHelper.For("ConfirmOk")">@SuccessText</MudButton>
    </DialogActions>
</MudDialog>
